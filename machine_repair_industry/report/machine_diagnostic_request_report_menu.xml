			<field name="dpi">90</field>
		</record>		

		<report id="machine_diagnostic_request_report_id"
				string="Machine Diagnostic Request"
				model="machine.diagnose"
				report_type="qweb-pdf"
				file="machine_repair_industry.machi_diagn_req_temp_id"
				name="machine_repair_industry.machi_diagn_req_temp_id"
