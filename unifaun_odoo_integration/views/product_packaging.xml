<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="unifaun_package_code_form_view" model="ir.ui.view">
            <field name="name">Unifaun Package Code</field>
            <field name="model">product.packaging</field>
            <field name="inherit_id" ref="delivery.product_packaging_delivery_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='shipper_package_code']" position="after">
                    <field name="unifaun_package_code" />
                </xpath>
            </field>
        </record>
    </data>
</odoo>